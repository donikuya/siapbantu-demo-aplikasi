<h1>New kontak</h1>

<%= error_messages_for :kontak %>

<% form_for(@kontak) do |f| %>
  <p>
    <b>Nama</b><br />
    <%= f.text_field :nama %>
  </p>

  <p>
    <b>Email</b><br />
    <%= f.text_field :email %>
  </p>

  <p>
    <b>Pesan</b><br />
    <%= f.text_area :pesan %>
  </p>

  <p>
    <b>Tanggal</b><br />
    <%= f.datetime_select :tanggal %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', kontaks_path %>